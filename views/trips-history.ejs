<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="/css/history.css">
    <title>Trips</title>
</head>
<body>
    <%- include('navbar.ejs') %>
    <a id="title"> Trips </a>
    <div class="trips">
        <div class="cards">
            <div class="past_Card">
                <% dates.forEach((date) => {%>
                <h4><strong><%= date.place %></strong></h4>
                <h5><%= date.date %></h5>
                <p><%= date.stay %></p>

                
                }); %>
            </div>
        </div>
    </div>


    <ul>
        <% mascots.forEach(function(mascot) { %>
          <li>
            <strong><%= mascot.name %></strong>
            representing <%= mascot.organization %>,
            born <%= mascot.birth_year %>
          </li>
        <% }); %>
      </ul>



      <ul>
        <% data.hobbies.forEach((item)=>{%>
            <li>
                <%=item%>
            </li>
            <%});%>
    </ul>
</body>
</html>